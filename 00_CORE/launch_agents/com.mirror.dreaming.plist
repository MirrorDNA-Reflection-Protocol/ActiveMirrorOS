<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.mirror.dreaming</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>/Users/mirror-admin/Documents/GitHub/active-mirror-identity/services/dreaming_daemon.py</string>
    </array>
    <key>StartCalendarInterval</key>
